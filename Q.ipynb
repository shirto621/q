        "def bus_counter(lst):\n",
        "  total_num=0\n",
        "  for item in lst:\n",
        "   total_num+=item[0]-item[1]\n",
        "   if total_num<0:\n",
        "    return -1\n",
        "  return total_num\n",
        "\n",
        "lst=[[2,0],[3,2],[6,4],[1,3],[0,2]]\n",
        "result=bus_counter(lst)\n",
        "print(result)"
